<jaxb:bindings
        xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        version="3.0">

  <jaxb:bindings
          schemaLocation="../../mc160_wsdl/target/resources/wsdl/MedicineCard-inline_2022_01_01.wsdl#types?schema3"
          node="/xs:schema">

    <jaxb:bindings
            node="//xs:complexType[@name='ListDispensingPatientsRequestType']/xs:sequence/xs:choice/xs:element[@name='IncludeDoseDispensingPatients' and not(@minOccurs)]">
      <jaxb:property name="doseDispensingOnly"/>
    </jaxb:bindings>

    <jaxb:bindings
            node="//xs:complexType[@name='ListDispensingPatientsRequestType']/xs:sequence/xs:choice/xs:sequence/xs:element[@name='IncludeDoseDispensingPatients' and @minOccurs='0']">
      <jaxb:property name="doseDispensingWithManual"/>
    </jaxb:bindings>

  </jaxb:bindings>

</jaxb:bindings>
