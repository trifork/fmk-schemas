    <include schemaLocation="importsFrom20150601E3.xsd"/>

	<element name="PackagedDrugSubstitution" type="medicinecard20150601E3:PackagedDrugSubstitutionType">
		<annotation>
			<documentation xml:lang="en-GB">Selection of packaged drugs for a dose dispensing period</documentation>
			<documentation xml:lang="da-DK">Valg af l√¶gemidler der skal pakkes i en given DD periode</documentation>
