		</annotation>
	</element>
	<simpleType name="PatientOrganisationRelationTypeType">
		<union memberTypes="medicinecard20120901:PredefinedPatientOrganisationRelationTypeType medicinecard20120901:UndefinedPatientOrganisationRelationTypeType"/>
	</simpleType>
	<simpleType name="PredefinedPatientOrganisationRelationTypeType">
		<restriction base="string">
			<enumeration value="Visiteret til hjemmesygeplejen"/>
		</restriction>
	</simpleType>
	<simpleType name="UndefinedPatientOrganisationRelationTypeType">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="200"/>
		</restriction>
	</simpleType>
</schema>
